{
    "SerialPorts":
    {
        "Projector": "/dev/ttyUSB3",
        "Screen": "/dev/ttyUSB2"
    },
    "ScreenLowerTime": 15200,
    "Serilog":
    {
        "MinimumLevel":
        {
            "Default": "Debug",
            "Override":
            {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "WriteTo":
        [
            {
                "Name": "File",
                "Args":
                { 
                    "path": "Automation.Log",
                    "rollingInterval": "Day"
                }
            }
        ]
    }
}